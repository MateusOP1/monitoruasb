//=================================================
void buzzer ()
{
  

    for (frequencia = 150; frequencia < 1800; frequencia += 1) 
  {
    tone(Pinofalante, frequencia, tempo); 
    delay(1);
  }
  for (frequencia = 1800; frequencia > 150; frequencia -= 1) 
  {
    tone(Pinofalante, frequencia, tempo); 
    delay(1);
  }
}
void setup() 
{
  pinMode(boia,INPUT_PULLUP);

  pinMode(Pinofalante, OUTPUT);
  
  
  Serial.begin(115200);
  sensors.begin();
  
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C); 
   
    display.clearDisplay();
    display.drawBitmap(0, 5, logoespk8 , 128, 64, 1);
    display.setTextColor(WHITE);
    display.setTextSize(1);
    display.setCursor(10,55);
    display.println("Conectando...");
    display.display();
  
  
 
  delay(1500);

}

void loop() 
{

    estadoboia = digitalRead(boia);
    Serial.print(estadoboia);
    
    while(estadoboia == 1) {
    buzzer ();
    display.clearDisplay();
    display.drawBitmap(0, 0, pare1 , 128, 64, 1);
    display.setTextColor(WHITE);
    display.display();
    delay(500);
    display.clearDisplay();
    display.drawBitmap(0, 0, pare2 , 128, 64, 1);
    display.setTextColor(WHITE);
    display.display();
    delay(500);
    } 
    
  gas();
  dalas();

  
    
}
